anim()
{
sleep 88;
turn sleeve to y-axis <-14.000000> now;
turn sleeve to x-axis <349.000000> now;
turn flare1 to y-axis <2.000000> now;
turn flare1 to x-axis <-2.000000> now;
turn flare1 to z-axis <74.000000> now;
}
